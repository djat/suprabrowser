		                    <groupId>local.group</groupId>
		                    <version>1.0.0</version>
		                    <localRepositoryPath>${basedir}/local-repo</localRepositoryPath>
		                </configuration>
		            </execution>
		             <execution>
		                <id>install-jar8</id>
		                <phase>validate</phase>
		                <goals>
		                    <goal>install-file</goal>
		                </goals>
		                <configuration>
		                	<file>lib/swt-4.4M5-win32-win32-x86.jar</file>
		                    <packaging>jar</packaging>
		                    <artifactId>swt-4.4M5.local.Id</artifactId>
		                    <groupId>local.group</groupId>
		                    <version>1.0.0</version>
		                    <localRepositoryPath>${basedir}/local-repo</localRepositoryPath>
		                </configuration>
		            </execution>
		        </executions>
		    </plugin>
