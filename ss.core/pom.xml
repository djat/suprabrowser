	<build>
		<plugins>
			<plugin>
		        <groupId>org.apache.maven.plugins</groupId>
		        <artifactId>maven-install-plugin</artifactId>
		        <version>2.5.1</version>
		        <executions>
		            <execution>
		                <id>install-jar1</id>
		                <phase>validate</phase>
		                <goals>
		                    <goal>install-file</goal>
		                </goals>
		                <configuration>
		                	<file>lib/ss.lab.dm3.jar</file>
		                    <packaging>jar</packaging>
		                    <artifactId>ss.lab.dm3.local.Id</artifactId>
		                    <groupId>local.group</groupId>
		                    <version>1.0.0</version>
		                    <localRepositoryPath>${basedir}/local-repo</localRepositoryPath>
		                </configuration>
		            </execution>
		            <execution>
		                <id>install-jar2</id>
		                <phase>validate</phase>
		                <goals>
		                    <goal>install-file</goal>
		                </goals>
		                <configuration>
		                	<file>lib-win32/ss/ss.os.utils.win32.jar</file>
		                    <packaging>jar</packaging>
		                    <artifactId>ss.os.utils.win32.local.Id</artifactId>
		                    <groupId>local.group</groupId>
		                    <version>1.0.0</version>
		                    <localRepositoryPath>${basedir}/local-repo</localRepositoryPath>
		                </configuration>
		            </execution>
		            <execution>
		                <id>install-jar3</id>
		                <phase>validate</phase>
		                <goals>
		                    <goal>install-file</goal>
		                </goals>
		                <configuration>
		                	<file>lib/xulrunner/MozillaInterfaces.jar</file>
		                    <packaging>jar</packaging>
		                    <artifactId>MozillaInterfaces.local.Id</artifactId>
		                    <groupId>local.group</groupId>
		                    <version>1.0.0</version>
		                    <localRepositoryPath>${basedir}/local-repo</localRepositoryPath>
		                </configuration>
		            </execution>
		            <execution>
		                <id>install-jar4</id>
		                <phase>validate</phase>
		                <goals>
		                    <goal>install-file</goal>
		                </goals>
		                <configuration>
		                	<file>lib/vcard4j-1_1_3.jar</file>
		                    <packaging>jar</packaging>
		                    <artifactId>vcard4j.local.Id</artifactId>
		                    <groupId>local.group</groupId>
		                    <version>1.0.0</version>
		                    <localRepositoryPath>${basedir}/local-repo</localRepositoryPath>
		                </configuration>
		            </execution>
		            <execution>
		                <id>install-jar5</id>
		                <phase>validate</phase>
		                <goals>
		                    <goal>install-file</goal>
		                </goals>
		                <configuration>
		                	<file>lib/swtdock.jar</file>
		                    <packaging>jar</packaging>
		                    <artifactId>swtdock.local.Id</artifactId>
		                    <groupId>local.group</groupId>
		                    <version>1.0.0</version>
		                    <localRepositoryPath>${basedir}/local-repo</localRepositoryPath>
		                </configuration>
		            </execution>
		            <execution>
		                <id>install-jar6</id>
		                <phase>validate</phase>
		                <goals>
		                    <goal>install-file</goal>
		                </goals>
		                <configuration>
		                	<file>lib/uif-1.4.1.jar</file>
		                    <packaging>jar</packaging>
		                    <artifactId>uif.local.Id</artifactId>
		                    <groupId>local.group</groupId>
		                    <version>1.0.0</version>
		                    <localRepositoryPath>${basedir}/local-repo</localRepositoryPath>
		                </configuration>
		            </execution>
		            <execution>
		                <id>install-jar7</id>
		                <phase>validate</phase>
		                <goals>
		                    <goal>install-file</goal>
		                </goals>
		                <configuration>
		                	<file>lib/bouncy.jar</file>
